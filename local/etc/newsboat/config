browser "termux-open %u"
cache-file ~/.local/var/lib/newsboat/cache.db
notify-program "notify-send"
notify-always yes
refresh-on-startup yes

bind-key j next
bind-key k prev
bind-key J next-feed
bind-key K prev-feed

color background        color250 black
color info              white    blue
color listnormal        color250 black
color listfocus         white    color240
color listnormal_unread green    black
color listfocus_unread  color46  color240
color article           color250 black
